    "tokenColors": [
        {
            "name": "Comments",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": { "foreground": "#57534E", "fontStyle": "italic" }
        },
        {
            "name": "Strings",
            "scope": ["string", "string.quoted"],
            "settings": { "foreground": "#C9956B" }
        },
        {
            "name": "Numbers",
            "scope": ["constant.numeric"],
            "settings": { "foreground": "#D4A97A" }
        },
        {
            "name": "Constants",
            "scope": ["constant", "constant.language"],
            "settings": { "foreground": "#B89878" }
        },
        {
            "name": "Keywords",
            "scope": ["keyword", "storage.type", "storage.modifier"],
            "settings": { "foreground": "#B87333" }
        },
        {
            "name": "Control Keywords",
            "scope": ["keyword.control"],
            "settings": { "foreground": "#D4A054", "fontStyle": "italic" }
        },
        {
            "name": "Operators",
            "scope": ["keyword.operator"],
            "settings": { "foreground": "#A8A29E" }
        },
        {
            "name": "Functions",
            "scope": ["entity.name.function", "support.function"],
            "settings": { "foreground": "#E0C8A8" }
        },
        {
            "name": "Function Parameters",
            "scope": ["variable.parameter"],
            "settings": { "foreground": "#C8BEB0" }
        },
        {
            "name": "Classes & Types",
            "scope": ["entity.name.type", "entity.name.class", "support.class", "support.type"],
            "settings": { "foreground": "#D4B896" }
        },
        {
            "name": "Interfaces",
            "scope": ["entity.name.type.interface"],
            "settings": { "foreground": "#C9A880" }
        },
        {
            "name": "Variables",
            "scope": ["variable", "variable.other"],
            "settings": { "foreground": "#E7E0D8" }
        },
        {
            "name": "Properties",
            "scope": ["variable.other.property", "support.variable.property"],
            "settings": { "foreground": "#BEB0A0" }
        },
        {
            "name": "Decorators / Annotations",
            "scope": ["meta.decorator", "punctuation.decorator"],
            "settings": { "foreground": "#B87333", "fontStyle": "italic" }
        },
        {
            "name": "Tags (HTML/XML)",
            "scope": ["entity.name.tag"],
            "settings": { "foreground": "#C9956B" }
        },
        {
            "name": "Tag Attributes",
            "scope": ["entity.other.attribute-name"],
            "settings": { "foreground": "#B09880", "fontStyle": "italic" }
        },
        {
            "name": "CSS Properties",
            "scope": ["support.type.property-name.css"],
            "settings": { "foreground": "#BEB0A0" }
        },
        {
            "name": "CSS Values",
            "scope": ["support.constant.property-value.css"],
            "settings": { "foreground": "#D4B896" }
        },
        {
            "name": "Regex",
            "scope": ["string.regexp"],
            "settings": { "foreground": "#C9956B" }
        },
        {
            "name": "Escape Characters",
            "scope": ["constant.character.escape"],
            "settings": { "foreground": "#B87333" }
        },
        {
            "name": "Markdown Heading",
            "scope": ["heading.1.markdown", "heading.2.markdown", "heading.3.markdown", "markup.heading"],
            "settings": { "foreground": "#B87333", "fontStyle": "bold" }
        },
        {
            "name": "Markdown Bold",
            "scope": ["markup.bold"],
            "settings": { "foreground": "#F5F0EB", "fontStyle": "bold" }
        },
        {
            "name": "Markdown Italic",
            "scope": ["markup.italic"],
            "settings": { "foreground": "#D6D0C8", "fontStyle": "italic" }
        },
        {
            "name": "Markdown Code",
            "scope": ["markup.inline.raw"],
            "settings": { "foreground": "#C9956B" }
        },
        {
            "name": "Markdown Link",
            "scope": ["markup.underline.link"],
            "settings": { "foreground": "#7EA8BE" }
        },
        {
            "name": "JSON Keys",
            "scope": ["support.type.property-name.json"],
            "settings": { "foreground": "#C9956B" }
        },
        {
            "name": "YAML Keys",
            "scope": ["entity.name.tag.yaml"],
            "settings": { "foreground": "#C9956B" }
        },
        {
            "name": "Punctuation",
            "scope": ["punctuation"],
            "settings": { "foreground": "#78716C" }
        },
        {
            "name": "Invalid / Deprecated",
            "scope": ["invalid", "invalid.deprecated"],
            "settings": { "foreground": "#C47070" }
        }
    ],
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "function": "#E0C8A8",
        "function.declaration": "#E0C8A8",
        "function.defaultLibrary": "#D4B896",
        "variable": "#E7E0D8",
        "variable.readonly": "#D4C8B8",
        "parameter": "#C8BEB0",
        "property": "#BEB0A0",
        "property.readonly": "#B0A090",
        "class": "#D4B896",
        "interface": "#C9A880",
        "enum": "#C9956B",
        "enumMember": "#BEA080",
        "type": "#D4B896",
        "namespace": "#B89878",
        "decorator": "#B87333",
        "string": "#C9956B",
        "number": "#D4A97A",
        "keyword": "#B87333",
        "comment": "#57534E",
        "operator": "#A8A29E",
        "regexp": "#C9956B"
    }
}
